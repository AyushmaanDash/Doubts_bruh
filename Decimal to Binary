#include<iostream>
#include<cmath>
using namespace std;

int main() 
{
	int num;
    cin>>num;
    
    if((num == 0) || (num == 1))
    {
        cout<<num;
    	return 0;
    }
    
    // checking the number of bits required
    
    int bits = ceil(log2(num+1));
    
    int binary = 0;
    int i =0;
    
    while(bits)
    {
        int rem = num%2;
        binary = binary + (rem * pow(10,i)) ;
        num = num/2;
        i++;
        bits--;
    }
    
    cout<<binary;
    
    return 0;
}

